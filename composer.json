{
  "name": "drupal/islandora_mirador",
  "description": "Islandora support for Mirador viewer",
  "type": "drupal-module",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": ["Drupal", "Islandora"],
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/Islandora/islandora_mirador",
  "support": {
    "issues": "https://github.com/Islandora/documentation/issues"
  },
  "replace": {
    "islandora/islandora_mirador": "self.version"
  },
  "require": {
    "drupal/islandora": "^2"
  },
  "conflict": {
    "drupal/core": "<=8",
    "islandora/islandora_defaults": "<3"
  }
}
